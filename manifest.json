{
  "author": "Jake, Inc.",
  "name": "Kolesa/Krisha Dark Theme",
  "description": "Enjoy dark theme in popular site of Kazakhstan",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "scripting"
  ],
  "icons": {
    "16": "icons/logo16.png",
    "32": "icons/logo32.png",
    "48": "icons/logo48.png",
    "128": "icons/logo128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://kolesa.kz/*",
        "https://krisha.kz/*"
      ],
      "js": [
        "js/jquery-3.7.1.slim.min.js"
      ]
    },
    {
      "matches": [
        "https://kolesa.kz/*"
      ],
      "js": [
        "js/kolesa.js"
      ],
      "css": [
        "css/kolesa.css",
        "css/messages.css"
      ]
    },
    {
      "matches": [
        "https://krisha.kz/*"
      ],
      "js": [
        "js/krisha.js"
      ],
      "css": [
        "css/krisha.css",
        "css/messages.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "css/krisha.css",
    "css/messages.css",
    "css/kolesa.css",
    "js/kolesa.js",
    "js/krisha.js",
    "js/jquery-3.7.1.slim.min.js",
    "icons/logo.jpg"
  ]
}
