{
  "name": "Kolesa Dark Theme",
  "description": "Just for me!",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": [
        "https://*.kz/*"
      ],
      "js": [
        "js/jquery-3.7.1.slim.min.js"
      ]
    },
    {
      "matches": [
        "https://kolesa.kz/*"
      ],
      "js": [
        "js/kolesa.js"
      ],
      "css": [
        "css/kolesa.css",
        "css/messages.css"
      ]
    },
    {
      "matches": [
        "https://krisha.kz/*"
      ],
      "js": [
        "js/krisha.js"
      ],
      "css": [
        "css/krisha.css",
        "css/messages.css"
      ]
    }
  ]
}
